<%- include('../layout/header', {title: 'Dashboard'}) %>

<%- include('../layout/menu', {title1: 'ADD'}) %>

<% authentication.forEach(auth => { %>
    <article class="larg">
      <div>
        <h3><%= auth.name %>  <span class="entypo-down-open"></span></h3>
        <p>
          <span>
            <%= auth.email %>
          </span>
        </p>
        <p>
          <span>
            function: <%= auth.func %>
          </span>
        </p>
        <p>
            <span>
                <a href="/auth/<%= auth._id %>?_method=delete">Adicionar</a>
            </span>
        </p>
      </div>
    </article>
<% }) %>

  </main>
